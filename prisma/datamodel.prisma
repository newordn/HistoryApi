type Post{
    id: ID! @id
    title: String!
    description: String!
    image: String!
    createdAt: DateTime! @createdAt
    date:String!
    belongTo: History @relation(name:"PostHistory")
}
type History{
    id: ID! @id
    title: String!
    description:String!
    from: String!
    to:String!
    date:String!
    posts:[Post!]! @relation(name:"PostHistory")
}